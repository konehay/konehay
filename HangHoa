#include <iostream>

using namespace std;

class HangHoa {
protected:
	string Mahh;
	string name;
	string noisx;
	string color;
	int cost;
	int NgayNhapKho;
	int Quantity;
public:
	HangHoa() {
		Mahh = " ";
		name = " ";
		noisx = " ";
		color = " ";
		cost = 0;
		NgayNhapKho = 0;
		Quantity = 0;
	}
	HangHoa(string _mahh, string _name, string _noisx, string _color, int _cost, int _NgayNhapKho, int _Quantity) {
		Mahh = _mahh;
		name = _name;
		noisx = _noisx;
		color = _color;
		cost = _cost;
		NgayNhapKho = _NgayNhapKho;
		Quantity = _Quantity;
	}
	HangHoa(HangHoa& h) {
		Mahh = h.Mahh;
		name = h.name;
		noisx = h.noisx;
		color = h.color;
		cost = h.cost;
		NgayNhapKho = h.NgayNhapKho;
		Quantity = h.Quantity;
	}
	string getMahh(){
		return Mahh;
	}
	string getname() {
		return name;
	}
	string getnoisx() {
		return noisx;
	}
	string getcolor(){
		return color;
	}
	~HangHoa() {};

	friend istream& operator>>(istream& is, HangHoa& h) {
		
	}
};


int main() {


	return 0;
}

